@model JokeyModel

@{
    ViewData["Title"] = "Jokey`s profile";
}

<h1>Профиль</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            ФИО:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>

        <dt class = "col-sm-2">
            Дата рождения:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DOB)
        </dd>

        <dt class = "col-sm-2">
            Рейтинг:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Rating)
        </dd>
    </dl>
</div>
